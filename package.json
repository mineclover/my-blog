{
  "name": "my-blog",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start",
    "build": "docusaurus clear && docusaurus start && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "dd": "set \"GIT_USER=mineclover\" && yarn deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus clear && docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "sync": "git pull && npm run sync --prefix ./macro && npm i",
    "setup": "git clone https://github.com/mineclover/ObsidianMarkdown-to-docusaurus.git macro && npm install --prefix ./macro",
    "macro": "npm start --prefix ./macro",
    "macro-commit": "npm run commit --prefix ./macro",
    "upload": "git add . && git commit -m \"변경 사항 일괄 저장\" && git push && npm run upload --prefix ./macro"
  },
  "dependencies": {
    "@docusaurus/core": "2.4.0",
    "@docusaurus/preset-classic": "^2.4.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-playground": "file:.."
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.4.0",
    "@tsconfig/docusaurus": "^1.0.5",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
